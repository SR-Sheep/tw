		<typeAlias type="com.jyp.tw.vo.Contents" alias="Contents"/>
		<typeAlias type="com.jyp.tw.vo.ContentsProd" alias="ContentsProd"/>
		<typeAlias type="com.jyp.tw.vo.Delivery" alias="Delivery"/>
		<typeAlias type="com.jyp.tw.vo.DeliveryInfo" alias="DeliveryInfo"/>
		<typeAlias type="com.jyp.tw.vo.DeliveryType" alias="DeliveryType"/>
		<typeAlias type="com.jyp.tw.vo.Discount" alias="Discount"/>
