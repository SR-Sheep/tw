	<!-- mapper 경로 설정 -->
	<mappers>
		<mapper resource="com/jyp/tw/mapper/admin.xml"></mapper>
		<mapper resource="com/jyp/tw/mapper/cartlist.xml"></mapper>
		<mapper resource="com/jyp/tw/mapper/category.xml"></mapper>
		<mapper resource="com/jyp/tw/mapper/contents.xml"></mapper>
		<mapper resource="com/jyp/tw/mapper/contentsProd.xml"></mapper>
