		<mapper resource="com/jyp/tw/mapper/discount.xml"></mapper>
		<mapper resource="com/jyp/tw/mapper/member.xml"></mapper>
		<mapper resource="com/jyp/tw/mapper/orderProd.xml"></mapper>
		<mapper resource="com/jyp/tw/mapper/payment.xml"></mapper>
		<mapper resource="com/jyp/tw/mapper/pointHis.xml"></mapper>
		<mapper resource="com/jyp/tw/mapper/prodCategory.xml"></mapper>
		<mapper resource="com/jyp/tw/mapper/prodOption.xml"></mapper>
